                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="buttonClient" layoutX="160.0" layoutY="225.0" mnemonicParsing="false" onAction="#navigateToClient" prefHeight="338.0" prefWidth="604.0" style="-fx-border-color: #584062; -fx-border-radius: 10;" text="partie client">
