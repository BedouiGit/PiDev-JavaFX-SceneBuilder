<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="800.0" prefWidth="215.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SideBarDashboard">
    <children>
        <AnchorPane fx:id="side_ankerpane" prefHeight="804.0" prefWidth="215.0" styleClass="side_ankerpane" stylesheets="@../../style.css">
            <children>
                <Pane fx:id="inner_pane" layoutX="13.0" layoutY="30.0" prefHeight="67.0" prefWidth="190.0" styleClass="inner_pane" stylesheets="@../../style.css">
                    <children>
                        <ImageView fitHeight="74.0" fitWidth="74.0" layoutX="33.0" layoutY="-3.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../images/nftlogo.png" />
                            </image>
                        </ImageView>
                        <Pane fx:id="most_inner_pane" layoutX="150.0" layoutY="2.0" prefHeight="64.0" prefWidth="33.0" styleClass="most_inner_pane" stylesheets="@../../style.css">
                            <children>
                                <ImageView fitHeight="45.0" fitWidth="49.0" layoutX="-8.0" layoutY="10.0">
                                    <image>
                                        <Image url="@../../images/dot.png" />
                                    </image>
                                </ImageView>
                            </children>
                        </Pane>
                    </children>
                </Pane>
                <Button fx:id="btn_workbench" layoutX="34.0" layoutY="147.0" mnemonicParsing="false" onAction="#NavigateToUserManagement" prefHeight="46.0" prefWidth="127.0" styleClass="btn_workbench" stylesheets="@../../style.css" text="Home" textFill="WHITE">
                    <font>
                        <Font name="Calibri Bold" size="15.0" />
                    </font>
               <graphic>
                  <ImageView fitHeight="28.0" fitWidth="27.0">
                     <image>
                        <Image url="@../../images/home.png" />
                     </image>
                  </ImageView>
               </graphic>
                </Button>
                <Button layoutX="40.0" layoutY="226.0" mnemonicParsing="false" onAction="#NavigateToNFT" prefHeight="50.0" prefWidth="98.0" styleClass="btn_workbenchCategories" stylesheets="@../../style.css" text="NFT" textFill="WHITE">
                    <font>
                        <Font name="Arial Black" size="15.0" />
                    </font>
                    <graphic>
                        <ImageView fitHeight="32.0" fitWidth="37.0">
                            <image>
                                <Image url="@../../images/nft.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button layoutX="39.0" layoutY="277.0" mnemonicParsing="false" onAction="#NavigateToPublication" prefHeight="52.0" prefWidth="120.0" styleClass="btn_workbenchCategories" text="Articles" textFill="WHITE">
                    <font>
                        <Font name="Arial Black" size="15.0" />
                    </font>
                    <graphic>
                        <ImageView fitHeight="31.0" fitWidth="27.0">
                            <image>
                                <Image url="@../../images/nouvelles.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>



                <Button layoutX="44.0" layoutY="329.0" mnemonicParsing="false" onAction="#NavigateToNews" prefHeight="52.0" prefWidth="110.0" styleClass="btn_workbenchCategories" text="News" textFill="WHITE">
                    <font>
                        <Font name="Arial Black" size="15.0" />
                    </font>
                    <graphic>
                        <ImageView fitHeight="31.0" fitWidth="36.0">
                            <image>
                                <Image url="@../../images/news.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="btnProjects" layoutX="14.0" layoutY="381.0" mnemonicParsing="false" onAction="#NavigateToCategories" prefHeight="46.0" prefWidth="167.0" styleClass="btn_workbenchCategories" text="Projects" textFill="WHITE">
                    <font>
                        <Font name="Arial Black" size="15.0" />
                    </font>
                    <graphic>
                        <ImageView fitHeight="32.0" fitWidth="31.0">
                            <image>
                                <Image url="@../../images/cible.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="btnrProjects" layoutX="14.0" layoutY="479.0" mnemonicParsing="false" onAction="#NavigateToTags" prefHeight="46.0" prefWidth="167.0" styleClass="btn_workbenchCategories" text="Tags" textFill="WHITE">
                    <font>
                        <Font name="Arial Black" size="15.0" />
                    </font>
                    <graphic>
                        <ImageView fitHeight="32.0" fitWidth="31.0">
                            <image>
                                <Image url="@../../images/cible.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>

                <Button layoutX="31.0" layoutY="433.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="135.0" styleClass="btn_workbenchCategories" text="Charts" textFill="WHITE">
                    <font>
                        <Font name="Arial Black" size="15.0" />
                    </font>
                    <graphic>
                        <ImageView fitHeight="29.0" fitWidth="28.0">
                            <image>
                                <Image url="@../../images/une-analyse.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>

            </children>
        </AnchorPane>
    </children>
</VBox>
